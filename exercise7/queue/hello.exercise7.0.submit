## This jobs runs for SLEEP_TIME seconds, giving us time to figure out stuff about the queue 
## To submit:  condor_submit <submit file>

universe = vanilla

SLEEP_TIME = 300
EXIT_CODE = 255

executable = bin/sleepAndWake.sh

args = "$(SLEEP_TIME) 'Hello World from job $(CLUSTER).$(PROCESS)' $(EXIT_CODE)"

output = outdir/hello.$(CLUSTER).$(PROCESS).out
error  = queue/error/hello.$(CLUSTER).$(PROCESS).err
log    = queue/log/hello.$(CLUSTER).log

queue 5
